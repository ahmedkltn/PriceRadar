{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python Debugger: Mytek Scraper",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/scripts/scraper/mytek_scraper.py",
      "console": "integratedTerminal",
      "args": ["--max_pages", "2" ],
      "env": {
        "PYTHONPATH": "${workspaceFolder}/scripts"
      }
    }
  ]
}